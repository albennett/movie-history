{{#each this}}
<div class="col-sm-3 col-xs-12 poster-display-unit" key="{{@key}}" watchtoggle="{{watched}}" fave="{{rating}}">
	<div class="x-background">
		<a href="#" class="delete-button">
			<span key="{{@key}}">&times;</span>
		</a>
	</div>
	<div class="myposter" id="{{@key}}" key="{{@key}}">
		<img class="pull-right" key="{{@key}}" src="{{Poster}}" onError="
		this.onerror=null;$(this).parent().html('<h3>{{Title}}</h3>');">
	</div>
	<div class="poster-controls">
		<button class="movie-watch btn btn-default pull-right" key="{{@key}}" watched="{{watched}}">Watch</button>
		<div class="stars pull-right" watched="{{watched}}">
			<input id="{{@key}}" class="rating" data-min="0" data-max="5" data-step="1" data-show-clear="false" data-show-caption="false" data-size="xs" value="{{rating}}">
		</div>
	</div>
</div>
{{/each}}  